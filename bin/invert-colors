#!/usr/bin/env python
from __future__ import print_function, division
import sys

fname_in=sys.argv[1]
fname_out=sys.argv[2]

if fname_in == fname_out:
    raise ValueError("input and output are same file")

with open(fname_in) as fin:
    with open(fname_out,'w') as fout:

        for line in fin:

            ls=line.split()
            if len(ls) >= 4 and ls[3] in ['SetCFg','SetCBg']:
                r=float(ls[0])
                g=float(ls[1])
                b=float(ls[2])

                ls[0] = '%g' % (1.0-r,)
                ls[1] = '%g' % (1.0-g,)
                ls[2] = '%g' % (1.0-b,)

                line=' '.join(ls)

            fout.write(line)
