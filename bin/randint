#!/usr/bin/env python

import numpy as np
import argparse

parser = argparse.ArgumentParser()
parser.add_argument('-n', '--nrand', type=int, default=1)
parser.add_argument('-m', '--max', type=int, default=2**30-1)
parser.add_argument('--seed', type=int, default=None)

args = parser.parse_args()

rng = np.random.RandomState(args.seed)

for i in range(args.nrand):
    print(rng.randint(args.max))
