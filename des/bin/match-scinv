#!/usr/bin/env python
"""
match dg scat to scinv by tile
"""
from __future__ import print_function
import os,sys
import des
import argparse

parser = argparse.ArgumentParser(description=__doc__)
parser.add_argument("scat_vers", help="the scat version. corresponds to config/{version}.yaml.  E.g. scat-001")

   
def main():
    args = parser.parse_args()

    des.scat.match_scat(args.scat_vers)

main()
